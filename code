void draw(void)
{
  u8g.drawBitmapP( 0, 0, 16, 64, frame1);
}

void setup(void) {
}

void loop(void) {

 u8g.firstPage();
  do{
   draw();
 } while(u8g.nextPage());
 
 frame ++;
 if(frame>3)
 frame = 0;
delay(3000);
}

